  "dependencies": {
    "bcryptjs": "2.4.3",
    "concurrently": "4.1.2",
    "config": "3.2.2",
    "express": "4.17.1",
    "jsonwebtoken": "9.0.0",
    "mongoose": "5.7.5",
    "react-router-dom": "5.0.1"
  },
  "devDependencies": {
    "i": "0.3.6",
    "mocha": "6.2.0",
    "nodemon": "1.19.2"
  }
}
