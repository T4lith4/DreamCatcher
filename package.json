{
  "name": "dream_catcher",
  "version": "1.0.0",
  "description": "DreamCatcher with JWT tokens",
  "main": "server.js",
  "engines": {
    "npm": "6.9.0",
    "node": "10.16.3"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "test": "echo \"Error: no test specified\" && exit 1",
